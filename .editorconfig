root = true

[*]
charset=utf-8
end_of_line=lf
insert_final_newline=true
indent_style=space
indent_size=4

[*.html]
indent_size=2

[*.py]
line_length=120
known_standard_library=dataclasses
known_first_party=my_iot
default_section=THIRDPARTY
not_skip=__init__.py
use_parentheses=true
include_trailing_comma=true
multi_line_output=3

[Makefile]
indent_style = tab
